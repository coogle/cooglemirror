{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/coogle/PHPoAuthLib"
        }
    ],
	"require": {
		"laravel/framework": "4.2.*",
		"lusitanian/oauth": "dev-master",
		"guzzlehttp/guzzle": "^6.2",
		"cooglemirror/drivetime": "dev-master",
		"cooglemirror/clock" : "dev-master",
		"cooglemirror/headlines" : "dev-master",
		"cooglemirror/smappee" : "dev-master",
		"cooglemirror/weather" : "dev-master"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
		"psr-0": {
         "Cooglemirror": "app/Cooglemirror"
    	}
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
